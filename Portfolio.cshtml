@model IEnumerable<Project>

<!DOCTYPE html>
<html>
<head>
    <title>Projects</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            background-color: #f5f5f5;
            color: #333;
            margin: 20px;
        }

        h2 {
            color: #4285f4;
        }

        .project {
            background-color: #fff;
            border: 1px solid #ddd;
            border-radius: 5px;
            padding: 10px;
            margin-bottom: 10px;
        }

        h3 {
            color: #555;
        }

        p {
            margin-bottom: 0;
        }

        .technologies {
            color: #777;
        }
    </style>
</head>
<body>
    <h2>Projects</h2>

    @foreach (var project in Model)
    {
        <div class="project">
            <h3>@project.Name</h3>
            <p>@project.Description</p>
            <p class="technologies">Technologies Used: @project.TechnologiesUsed</p>
        </div>
    }
</body>
</html>
